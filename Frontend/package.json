{
  "name": "toutiao-frontend",
  "version": "1.14.17",
  "description": "仿头条",
  "main": "app/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress && npm run node",
    "build:devdll": "cross-env NODE_ENV=development webpack --progress --config webpack.dll.config.js",
    "build:prodll": "cross-env NODE_ENV=production webpack --progress --config webpack.dll.config.js",
    "eslint": "eslint \"./app\" --fix --format \"./node_modules/eslint-formatter-pretty\" --ignore-pattern \"*.min.js\";exit 0",
    "generate": "plop --plopfile generator/index.js",
    "major": "npm --no-git-tag-version version major && npm run build",
    "minor": "npm --no-git-tag-version version minor && npm run build",
    "node": "node node.config.js",
    "patch": "npm --no-git-tag-version version patch && npm run build",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --progress",
    "stylelint": "stylelint ./app/**/*.scss --syntax scss --fix;exit 0"
  },
  "keywords": [
    "qingmooc",
    "mooc",
    "react",
    "webpack"
  ],
  "license": "MIT",
  "devDependencies": {
